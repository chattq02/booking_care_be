  "scripts": {
    "dev": "npx nodemon",
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "db:merge": "ts-node npx ts-node ./src/scripts/merge-prisma.ts",
    "db:generate": "ts-node src/scripts/merge-prisma.ts && npx prisma generate",
    "db:migrate": "ts-node src/scripts/merge-prisma.ts && ts-node src/scripts/migrate-auto.ts",
    "db:remove": "npx prisma migrate reset",
    "db:format": "npx prisma format"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
